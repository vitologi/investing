    "**/*.spec.{js,jsx,ts,tsx}",
    "**/tests/**",
    "**/__tests__/**",
    "**/*.d.ts"
  ],
  "ignoreUnimported": [
    "src/index.tsx",
    "src/modules/offline/service-worker.ts",
    "src/setupTests.ts"
  ],
  "ignoreUnused": [],
  "ignoreUnresolved": [
    "../../../types/theme"
  ]
}
