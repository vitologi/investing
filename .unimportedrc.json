{
  "entry": [
    "src/index.tsx",
    "src/setupTests.ts",
    "src/modules/offline/service-worker.ts"
  ],
  "extensions": [
    ".js",
    ".jsx",
    ".ts",
    ".tsx"
  ],
  "ignorePatterns": [
